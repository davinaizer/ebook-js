<nav class="text-center">
    <ul class="pagination">
        {{#each chapters}}
            <li>
                <a href="#chapter_{{@index}}" data-toggle="tooltip" title="{{title}}">{{@index}}</a>
                <div class="chapterguide-line"></div>
            </li>

        {{/each}}
    </ul>
</nav>

<button id="btn-prev-chapter" class="btn btn-primary pull-left" role="button">
    <i class="fa fa-chevron-left"></i> Módulo Anterior
</button>

<button id="btn-next-chapter" class="btn btn-primary pull-right" role="button">Próximo Módulo
    <i class="fa fa-chevron-right"></i>
</button>
